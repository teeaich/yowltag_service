type Recorddata {
    id: ID!
    recordId: String!
    lat: Float!
    long: Float!
    timestamp: String!
    dataObject: String
    bgGeoConfig: String
}

type Query {
}

type Mutation {
    createRecorddata(
        recordId: String!
        long: Float!
        lat: Float!
        dataObject: String!
        bgGeoConfig: String!
    ): Recorddata
}
